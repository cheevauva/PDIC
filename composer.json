{
    "name": "cheevauva/pdic",
    "type": "library",
    "description": "Property Dependency Injection Container",
    "keywords": ["property dependency injection", "dependency injection", "container"],
    "homepage": "https://github.com/cheevauva/pdic",
    "license": "MIT",
    "authors": [
        {
            "name": "Fedor Kazin",
            "email": "japanxt@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "php": ">=5.6",
        "phpunit/phpunit": "5.6.3",
        "pimple/pimple": "v3.2.3"
    },
    "autoload": {
        "psr-4": {
            "PDIC\\": "src/PDIC"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "PDICTest\\": "tests"
        }
    }
}
